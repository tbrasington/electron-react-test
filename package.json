{
  "name": "electron-reactsketchapp",
  "version": "1.0.0",
  "description": "A minimal Electron application to test react-sketchapp",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "render": "skpm-build --run",
    "render2": "skpm-build --run --manifest=src/manifest2.json"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "react-sketchapp"
  ],
  "devDependencies": {
    "electron": "^1.8.4"
  },
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "electron-test",
    "manifest": "src/manifest.json",
    "main": "electron-test.sketchplugin"
  },
  "dependencies": {
    "@skpm/builder": "^0.5.4",
    "react": "^16.3.2",
    "react-sketchapp": "^1.1.0",
    "react-test-renderer": "^16.3.2"
  }
}
